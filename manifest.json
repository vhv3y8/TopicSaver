{
  "name": "Topic Saver",
  "version": "0.1",
  "description": "Chrome Extension extended bookmark system.",
  "manifest_version": 2,

  "permissions": ["storage"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup/popup.html"
  }
}
